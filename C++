class Solution {
public:
    int majorityElement(vector<int>& nums) {
int n = nums.size();
        map<int,int> mp; 

        for(int i =0;i<n;i++){
            mp[nums[i]]++;


        }
            for(auto mp:mp)
            if(mp.second>(n/2))
            return mp.first;
        
    return -1;
    }

};
